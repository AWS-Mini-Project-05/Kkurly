<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.drg/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MainDAO">
	<!-- 쿼리 작성 영역 -->
	<select id="getProdList" parameterType="String" resultType="prod">
		SELECT * 
		FROM T_PROD
		WHERE PROD_CgCd = #{cd}
	</select>
	
</mapper>